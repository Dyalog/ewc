 JSWCUpdateScroll args;e;scores;cols;fcol;pass;top;left
 Event←⎕NS''
 e←Event.Event←⎕NS''
 e.(ID EventName Info)←(2↑args),⊂2↓args
 ⎕JSON Event

 :Select 1⊃args
 :CaseList 'F1.UPDOWN' 'F1.LEFTRIGHT' ⍝ Scroll
     top←'F1.UPDOWN' ∆WG 'THUMB'
     left←'F1.LEFTRIGHT' ∆WG 'THUMB'
     'F1.VGRID' ∆WS ('Values' ((⍕¨top+⍳n)∘.,'/',¨⍕¨left+⍳n))('RowTitles' ('Row '∘,¨⍕¨top+⍳n))('ColTitles' ((↓'ZI4' ⎕FMT ⍪left+⍳n)))
 :Else
     ⎕←'Unexpected message: ',args
 :EndSelect
